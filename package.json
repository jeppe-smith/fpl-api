{
  "name": "fpl-api",
  "version": "3.0.4",
  "description": "A wrapper for Fantasy Premier League API",
  "author": "Jeppe Smith <mail@jeppesmith.dk>",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/jeppe-smith/fpl-api",
  "scripts": {
    "prepublish": "npm run build && git push",
    "test": "jest",
    "build": "tsc",
    "coverage": "jest --collectCoverage",
    "documentation:create": "./node_modules/.bin/documentation readme src/web -f md --parse-extension ts --require-extension ts --section=Usage --markdown-toc-max-depth=2 --sort-order=alpha",
    "prepare": "install-peers"
  },
  "dependencies": {
    "cross-fetch": "^3.1.4",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "optionalDependencies": {
    "@types/tough-cookie": "^4.0.0",
    "fetch-cookie": "^0.11.0",
    "form-data": "^4.0.0",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.5.10",
    "documentation": "^13.2.5",
    "install-peers-cli": "^2.2.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4"
  },
  "main": "dist/web/index.js",
  "types": "dist/web/index.d.ts",
  "exports": {
    ".": "./dist/web/index.js",
    "./node": "./dist/node/index.js"
  },
  "typesVersions": {
    "*": {
      "node": [
        "dist/node/index.d.ts"
      ]
    }
  }
}
